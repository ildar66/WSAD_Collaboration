<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://dtdserver/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.hps.july.rts.object.capacity" >
    <class name="Capacity" table="TN_Capacities"  >
        <id name="id" column="capacityid" type="integer" unsaved-value="0">
            <generator class="native"/>
        </id>
        <property name="name" column="capacityval" type="string"/>
        <property name="indexCap" column="index_cap" type="string"/>
    </class>
</hibernate-mapping>
