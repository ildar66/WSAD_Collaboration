<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://dtdserver/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.hps.april.auth.object" >
    
    <!-- typedef name="Role" class="JulyRole"/ -->
    
    <class name="JulyRole" table="roles" lazy="false">
        <id name="name" column="role" type="string"/>
        <property name="description" column="rolename" type="string"/>
        <property name="taskName" column="taskcomment" type="string"/>
    </class>
    
</hibernate-mapping>
