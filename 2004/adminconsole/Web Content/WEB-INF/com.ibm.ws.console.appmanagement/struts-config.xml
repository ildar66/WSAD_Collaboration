      <action name="uploadForm" path="/upload" type="com.ibm.ws.console.appmanagement.action.UploadAction">
        <forward name="reload" path="appmanagement.upload"/>
        <forward name="nodebrowse" path="appmanagement.remote.node.browse"/>
        <forward name="next" path="appmanagement.default.bindings"/>
        <forward name="cancel" path="console.content.main"/>
      </action>

      <action name="defaultBindingsForm" path="/defaultBindings" type="com.ibm.ws.console.appmanagement.action.DefaultBindingsAction">
        <forward name="previous" path="appmanagement.upload"/>
        <forward name="finish" path="appmanagement.install.options"/>
        <forward name="reload" path="appmanagement.default.bindings"/>
        <forward name="warnings" path="appmanagement.security.warnings"/>
      </action>

      <action name="showSecurityWarningsForm" path="/showSecurityWarnings" type="com.ibm.ws.console.appmanagement.action.ShowSecurityWarningsAction">
        <forward name="continue" path="appmanagement.install.options"/>
        <forward name="cancel" path="appmanagement.upload"/>
      </action>

